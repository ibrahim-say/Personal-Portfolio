{
    "manifest_version": 2,
    "name": "Your Extension",
    "version": "1.0",
    "web_accessible_resources": [
      "popup.js"
    ],
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
  }